{
  "name": "ts-debug-helper",
  "displayName": "TypeScript Debug Helper",
  "description": "TypeScript debugg helper for vscode",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Debuggers"
  ],
  "publisher": "cytd",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/shenxiyou/ts-debug.git"
  },
  "activationEvents": [
    "onDebug"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.17.15",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "vscode-debugadapter": "1.51.0",
    "vscode-debugprotocol": "1.51.0",
    "vscode-languageclient": "8.0.2"
  }
}
